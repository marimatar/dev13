            <ImageView fitHeight="95.0" fitWidth="182.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
            <Button alignment="BASELINE_LEFT" layoutX="6.0" layoutY="29.0" mnemonicParsing="false" onAction="#showHomeOp" prefHeight="71.0" prefWidth="182.0" style="-fx-background-color: black;" text="Home Logo" textFill="WHITE" visible="false" />
         </children>
      </AnchorPane>
   </children>
</HBox>
