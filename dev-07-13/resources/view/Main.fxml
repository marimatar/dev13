   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: black;">
         <children>
            <Button alignment="BASELINE_LEFT" layoutX="9.0" layoutY="125.0" mnemonicParsing="false" onAction="#showHomeOp" prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: black;" text="Home" textFill="WHITE" />
            <Button alignment="BASELINE_LEFT" layoutX="9.0" layoutY="165.0" mnemonicParsing="false" onAction="#showCategoryOp" prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: black;" text="Create Category" textFill="WHITE" />
            <Button alignment="BASELINE_LEFT" layoutX="9.0" layoutY="205.0" mnemonicParsing="false" onAction="#showLocationOp" prefHeight="31.0" prefWidth="182.0" style="-fx-background-color: black;" text="Create Storage Location" textFill="WHITE" />
            <Button alignment="BASELINE_LEFT" layoutX="9.0" layoutY="245.0" mnemonicParsing="false" onAction="#showCreateAssetOp" prefHeight="31.0" prefWidth="140.0" style="-fx-background-color: black;" text="Create Asset" textFill="WHITE" />
